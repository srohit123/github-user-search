{
  "endOfLine": "lf",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "bracketSpacing": true,
  "printWidth": 80,
  "arrowParens": "avoid",
  "importOrderSeparation": true,
  "importOrderParserPlugins": [
    "jsx",
    "typescript",
    "classProperties",
    "decorators-legacy"
  ]
}
